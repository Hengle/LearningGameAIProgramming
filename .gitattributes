# Auto detect text files and perform LF normalization
* text=auto
media/shaders filter=lfs diff=lfs merge=lfs -text
media/textures filter=lfs diff=lfs merge=lfs -text
media/animations filter=lfs diff=lfs merge=lfs -text
media/fonts filter=lfs diff=lfs merge=lfs -text
media/materials filter=lfs diff=lfs merge=lfs -text
media/models filter=lfs diff=lfs merge=lfs -text
media/particle filter=lfs diff=lfs merge=lfs -text
media/programs filter=lfs diff=lfs merge=lfs -text
